<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ServerAndChannelController">
   <children>
      <Pane fx:id="kickBtnPopup" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onMouseClicked="#onKickClicked" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #202225;">
         <children>
            <Label layoutX="14.0" layoutY="10.0" text="Kick" textFill="#ed4245">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane fx:id="giveRoleBtnPopup" layoutX="10.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onMouseClicked="#onGiveRoleClicked" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #202225;">
         <children>
            <Label layoutX="14.0" layoutY="10.0" text="Give Role" textFill="#ed4245">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane fx:id="infoBtnPopup" layoutX="10.0" layoutY="60.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onMouseClicked="#onInfoClicked" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #202225;">
         <children>
            <Label layoutX="14.0" layoutY="10.0" text="Info" textFill="#ed4245">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</VBox>
